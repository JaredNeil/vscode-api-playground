{
  "name": "vscode-settings-cycler",
  "displayName": "Settings Cycler",
  "description": "Allows cycling through vscode settings using keyboard shortcuts",
  "version": "0.0.2",
  "publisher": "hoovercj",
  "engines": {
    "vscode": "^1.10.0"
  },
  "author": {
    "name": "Cody Hoover",
    "url": "www.codyhoover.com",
    "email": "vscode@codyhoover.com"
  },
  "bugs": {
    "url": "https://www.github.com/hoovercj/vscode-api-playground/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/hoovercj/vscode-api-playground"
  },
  "icon": "images/icon.png",
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "properties": {
        "settings.cycle": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "The name of the command to be created for this grouping of settings."
              },
              "values": {
                "type": "array",
                "description": "An array of setting groups to cycle through.",
                "items": {
                  "type": "object",
                  "description": "An object with the setting name as the object key and the setting value (string, boolean, object, etc.) as the object value."
                }
              },
              "overrideWorkspaceSettings": {
                "type": "boolean",
                "description": "When true, if there is a workspace setting set, it will be overridden. If false, and a workspace setting is present, the command will abort without changing settings.",
                "default": false
              }
            }
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "deep-equal": "^1.0.1"
  }
}
