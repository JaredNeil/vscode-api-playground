{
    "name": "helloworld",
    "displayName": "HelloWorld",
    "description": "Hello World extension produced by `yo code`",
    "version": "0.0.1",
    "publisher": "hoovercj",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "preview.html.preservefocus",
            "title": "Preview Html: Preserve Focus"
        }, {
            "command": "preview.html.dont.preservefocus",
            "title": "Preview Html: DON'T preserve focus"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}